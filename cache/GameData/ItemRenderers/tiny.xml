<?xml version="1.0" encoding="utf-8"?>
<ItemRenderer>
  <Data>
	<Sprite name="tie" fileName="game/custom_cosmetics1.rttex" textureSize="32" frame="31,0"/>

    <Animations>
        <SpriteAnimation name="TieAll" playOnAction="JUMP|FALL|IDLE|WALK" sprite="tie" animTime="400" autoPlay="true" isLoop="true">
		    <Frame>31,0</Frame>
	    </SpriteAnimation>
    </Animations>

    <RenderOptions>
      <Default>
        <Set name="UserNameOffset">-10</Set>
      </Default>
    </RenderOptions>
  </Data>

  <RenderRules>
	<OnDrawCustomOutline>
      <AvatarTransform class="Scale">
        <Set name="targetScale">0.15,0.15</Set>
      </AvatarTransform>
    </OnDrawCustomOutline>

	<RenderInFront>
    <SpriteRender name="tie" offset="0,0" blend="PREMULTIPLIED_ALPHA"/>
	</RenderInFront>
</RenderRules>
</ItemRenderer>
